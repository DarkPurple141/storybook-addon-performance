{
  "name": "storybook-performance-addon",
  "version": "0.1.0",
  "description": "A storybook addon to help better understand and debug performance for React components",
  "main": "dist/storybook-performance-addon.cjs.js",
  "types": "dist/storybook-performance-addon.d.ts",
  "module": "dist/storybook-performance-addon.esm.js",
  "sideEffects": false,
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian/storybook-addon-performance.git"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "keywords": [],
  "dependencies": {
    "@storybook/addons": "^5.3.18",
    "@storybook/components": "^5.3.18",
    "@xstate/react": "^0.8.1",
    "gzip-js": "^0.3.2",
    "tiny-invariant": "^1.1.0",
    "xstate": "^4.8.0"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.18",
    "@types/gzip-js": "^0.3.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "config": {
    "prettier_target": "src/**/*.{ts,js,jsx,md,json} test/**/*.{ts,js,jsx,md,json}"
  },
  "scripts": {
    "dev": "yarn start-storybook",
    "typescript:watch": "tsc --outDir ./dist --watch",
    "prettier:check": "yarn prettier --debug-check $npm_package_config_prettier_target",
    "prettier:write": "yarn prettier --write $npm_package_config_prettier_target",
    "test": "yarn jest",
    "build": "yarn build:clean && yarn build:dist && yarn build:typescript",
    "build:clean": "rimraf dist",
    "build:dist": "rollup -c",
    "build:typescript": "tsc ./src/index.ts --emitDeclarationOnly --declaration --outDir ./dist",
    "prepublishOnly": "yarn build"
  }
}
